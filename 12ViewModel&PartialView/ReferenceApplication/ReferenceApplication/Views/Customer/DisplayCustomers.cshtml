@model ReferenceApplication.ViewModel.CustomerVM
@using ReferenceApplication.Models;

<table>
    <tr>
        <td>Customer Code</td>
        <td>Customer Name</td>
    </tr>

    @foreach (CustomerModel o in Model.Customers)
            {
        <tr>
            <td>@o.CustomerCode</td>
            <td>@o.CustomerName</td>
        </tr>

    }
</table>



